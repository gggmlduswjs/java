package ch02.control;


/**
 * @날짜 :2025. 8. 20.
 * @작성자 : 전희연
 * @설명 : 
 */

public class Exam32 {
	public static void main(String[] args) {
		
		int x=1;		//초기값
		while(x<=10) {	//조건값 5<=10		
			if(x==5) {	// 5==5
				break;
			}
			
			System.out.println(x);	// 1 2 3 4
			++x; 		//증감값 5
		}
		System.out.println();
		
		
		
	}
}
