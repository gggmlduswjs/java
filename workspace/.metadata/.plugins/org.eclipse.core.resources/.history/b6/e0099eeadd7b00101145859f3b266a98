	public static void main(String[] args) {
		
		System.out.println("안녕하세요");
		System.out.print("hi");
		System.out.println("How are you");
		
		System.out.println("정수형(기본) 자료형");
		
		//byte(1byte,8bit), short(2byte,16bit), int(4byte,16bit), long(8byte,64bit)
		
		byte a=10;
	}

}
