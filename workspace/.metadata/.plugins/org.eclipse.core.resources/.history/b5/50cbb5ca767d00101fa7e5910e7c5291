package ch02.control;



/**
 * @날짜 :2025. 8. 20.
 * @작성자 : 전희연
 * @설명 : 제어문 - 반복문(while 문) / DB 연동 후 많이 사용
 */



public class Exam27 {

	public static void main(String[] args) {
		
		// 초기화, 조건값, 증감값 for(int i=1;i<10;i++){...}
		
		int x=1;	//초기값
		while(x<=10) {	//조건값
			System.out.print(x+"\t");
			++x; //증감값
		}

	}

}
