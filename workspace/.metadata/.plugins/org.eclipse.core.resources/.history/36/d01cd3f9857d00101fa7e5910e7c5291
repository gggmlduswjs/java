package ch02.control;



/**
 * @날짜 :2025. 8. 20.
 * @작성자 : 전희연
 * @설명 : 제어문 - 반복문(while, do~while 차이점)
 */


public class Exam29 {

	public static void main(String[] args) {
		
		int x=10;
		
		while(x>50) {	// 실행이 안된다.
			System.out.println("while x"+x);
		}

		do {
			System.out.println("do x :"+x);	// 10
		}while(x>50);	// x > 50
	}

}
