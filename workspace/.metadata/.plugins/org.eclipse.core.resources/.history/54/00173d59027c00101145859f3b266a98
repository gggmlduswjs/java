package ch01.dataType;

/*
 * 5. 비교 연산자 : <, >, <=, >=, ==, !=
 * 
 * */

public class Exam07 {

	public static void main(String[] args) {
		
		int a=10;
		int b=20;
		
		boolean c=a>b; 	// 10 > 20
		boolean d=a<b;	// 10 < 20
		boolean e=a>=b;	//10>=20
		boolean f=a<=b;
		
		System.out.println(c);
		System.out.println(d);
		System.out.println(e);
		System.out.println(f);



	}

}
