package ch02.control;

import java.util.Scanner;

/**
 * @날짜 :2025. 8. 21.
 * @작성자 : 전희연
 * @설명 : 
 */
public class Exam38 {
	
	public static void main(String[] args) {
	
	Scanner sc=new Scanner(System.in);
	
	//배열ㅇ;ㅣㅂ력
	int [] array= new int[5];
	
	for(int i=0;i<array.length;i++) {
		System.out.println((i+1)+"번째 수 입력");
		array[i]=sc.nextInt();
		
	}
	
	sc.close();
	
	}

}
