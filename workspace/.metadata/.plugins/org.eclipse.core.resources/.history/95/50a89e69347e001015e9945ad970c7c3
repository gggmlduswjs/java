import java.util.Scanner;

public class Exam38 {

	public static void main(String[] args) {
		
		int[] array=new int[5];	//[0]~[4] 20byte
		for(int i=0;i<array.length;i++) {
			System.out.println(array[i]);
		}

		int a;
		System.out.println(a);
	}

}
