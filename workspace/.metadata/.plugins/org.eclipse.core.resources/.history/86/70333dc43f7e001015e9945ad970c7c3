package ch03.array;



/**
 * @날짜 :2025. 8. 21.
 * @작성자 : 전희연
 * @설명 : 2차원 배열
 */
public class Exam41 {

	public static void main(String[] args) {
		int [][] array=new int[][] {{10, 20, 30},
									{40, 50, 60}};
									
		for(int i=0;i<2;i++) {		//0		1	
			for(int j=0;j<3;j++) {	//012	012
				System.out.print(array[i][j]+"\t");
			}
			System.out.println();
		}
		
		System.out.println("행의 수 : "+array.length);
	}

}
